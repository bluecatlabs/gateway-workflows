	<!ATTLIST data per-batch-history-records (true|false) #IMPLIED>
	<!ATTLIST data clear-sessions-on-success (true|false) #IMPLIED>
	<!ATTLIST data lock-parent-when-child-added-or-removed (true|false) #IMPLIED>

	<!ATTLIST data assume-empty-database (true|false) "false">
	<!ATTLIST data ignore-unsupported (true|false) "false">
	<!ATTLIST data existence-tracking-optimization (xml-only|fingerprint-and-xml|off) #IMPLIED>
	<!ATTLIST data fingerprinting (off|empty|bloom|detailed) #IMPLIED>
	<!ATTLIST data concurrent-preparation (true|false) #IMPLIED>

	<!ATTLIST data brief-stats-logging (true|false) #IMPLIED>
	<!ATTLIST data verbose-stats-reporting-period CDATA #IMPLIED>

	<!ATTLIST data log-skip-tree-children (true|false) #IMPLIED>
	<!ATTLIST data log-skip-tree-root (true|false) #IMPLIED>
	<!ATTLIST data log-skip-object (true|false) #IMPLIED>
	<!ATTLIST data log-create (true|false) #IMPLIED>
	<!ATTLIST data log-delete (true|false) #IMPLIED>
	<!ATTLIST data log-replace (true|false) #IMPLIED>
	<!ATTLIST data log-merge (true|false) #IMPLIED>
	<!ATTLIST data log-debug (true|false) #IMPLIED>

	<!ATTLIST data blocked-reaction-threshold-initial CDATA #IMPLIED>
	<!ATTLIST data blocked-reaction-threshold-expansion CDATA #IMPLIED>
	<!ATTLIST data blocked-reaction-threshold-contraction CDATA #IMPLIED>
	<!ATTLIST data incomplete-reaction-threshold-initial CDATA #IMPLIED>
	<!ATTLIST data incomplete-reaction-threshold-expansion CDATA #IMPLIED>
	<!ATTLIST data incomplete-reaction-threshold-contraction CDATA #IMPLIED>

